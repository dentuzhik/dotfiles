[user]
	name = Denis Tuzhik
	email = dentuzhik@gmail.com
[push]
	default = simple
[credential]
	helper = cache --timeout=28800
[help]
	autocorrect = 1
[core]
	editor = vim
	whitespace = -l
[color]
	ui = true
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	arc = !git add -A && git rebase --continue

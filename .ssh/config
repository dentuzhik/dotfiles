Host github.com
    HostName github.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/github_rsa
    ClearAllForwardings yes

Host gitlab.com
    HostName gitlab.com
    PreferredAuthentications publickey
    IdentityFile ~/.ssh/gitlab_rsa
    ClearAllForwardings yes

Host *
    User dentuzhik
    ForwardX11 yes
    ForwardX11Trusted yes
    ForwardAgent yes
    AddKeysToAgent yes
